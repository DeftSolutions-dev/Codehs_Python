speed(99)
#diamond
penup()
setposition(-100,70)
pendown()
begin_fill()
color("red")
circle(60,360,4)
end_fill()
#circle
penup()
setposition(100,70)
pendown()
begin_fill()
color("blue")
circle(60)
end_fill()
#pentagon
penup()
setposition(100,-150)
pendown()
begin_fill()
color("green")
circle(60,360,5)
end_fill()
#half cirlce
penup()
setposition(-130,-150)
pendown()
begin_fill()
color("yellow")
circle(60,180)
end_fill()
